
#
# enhancd
# https://github.com/b4b4r07/enhancd
#

source ~/.dotfiles/enhancd/init.sh

#
# zsh-autosuggestions
# https://github.com/zsh-users/zsh-autosuggestions
#

ZSH_AUTOSUGGEST_USE_ASYNC=1
ZSH_AUTOSUGGEST_BUFFER_MAX_SIZE=25      # disables suggestion for large buffers

source ~/.dotfiles/zsh-autosuggestions/zsh-autosuggestions.zsh

#
# history-search-multi-word
# https://github.com/zdharma/history-search-multi-word
#

source ~/.dotfiles/history-search-multi-word/history-search-multi-word.plugin.zsh

#
# spaceship prompt
# https://github.com/denysdovhan/spaceship-prompt
#

SPACESHIP_VI_MODE_SHOW=false
SPACESHIP_CHAR_SYMBOL="âžœ  "
